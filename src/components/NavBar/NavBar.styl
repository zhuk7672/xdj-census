.navBar
	position fixed
	z-index 2
	left 50%
	bottom 0
	width 750px
	// height 60px
	transform translateX(-50%)
	background-color #ffffff
	border-top 1px solid #ececec
	box-shadow 0 0 23px #fff
	ul
		width 100%
		height 100%
		display flex
		flex 1
	li
		position relative
		width 25%
		height 100%
		text-align center
		font-size 22px
		height 95px
		&:first-child
			background: url('http://s.sfddj.com/Public/static/img/newicon/orderAdmin/real-icon.png') center 23% no-repeat
			background-size 50px 50px
		&:nth-child(2)
			background: url('http://s.sfddj.com/Public/static/img/newicon/orderAdmin/yesterday-icon.png') center 23% no-repeat
			background-size 50px 50px
		&:nth-child(3)
			background: url('http://s.sfddj.com/Public/static/img/newicon/orderAdmin/sale-icon.png') center 23% no-repeat
			background-size 50px 50px
		&:last-child
			background: url('http://s.sfddj.com/Public/static/img/newicon/orderAdmin/real-icon.png') center 23% no-repeat
			background-size 50px 50px
		a
			position absolute
			top 0
			left 0
			width 100%
			padding-top 65px
			color rgb(88, 88, 88)
			&.acti
				color: red
	
			

